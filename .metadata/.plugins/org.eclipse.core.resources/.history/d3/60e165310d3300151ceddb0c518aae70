@CucumberOptions(
		features = {"src/test/java/com/marksandspencer/app"},
		plugin = {“pretty”,

“html:target/cucumber-html-report”,

“junit:target/cucumber-junit-report/allcukes.xml”},

tags = {“@Runme”}

)

public class Cukerunner {

}
